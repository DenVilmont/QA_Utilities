{
    "manifest_version": 3,
    "name": "QA Utilities",
    "version": "0.1.3.0",
    "description": "QA Utilities",
    "default_locale": "en",
    "background": {
        "service_worker": "/scripts/service-worker.js"
    },
    "action": {
        "default_title": "Click to open panel",
        "default_icon": "resourses/logo/logo.png"
    },
    "side_panel": {
        "default_path": "/pages/index.html"
    },
    "permissions": ["sidePanel"]
  }